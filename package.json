{
  "name": "about-me",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href=/about-me/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
